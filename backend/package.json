{
  "name": "money-tracker-express-backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.ts",
  "scripts": {
    "build": "babel --extensions .ts src -d build",
    "prestart": "npm link ../types",
    "start": "node build/server.js",
    "start:local": "dotenv -- ts-node -r tsconfig-paths/register src/server.ts",
    "dev": "dotenv -c dev -- ts-node-dev -r tsconfig-paths/register src/server.ts --respawn --transpile-only",
    "test": "dotenv -c test -- jest --runInBand",
    "test:watch": "dotenv -c test -- jest --watch --runInBand",
    "openapi": "node swagger",
    "openapi:test": "openapi-generator-cli generate -i openapi.yaml -g typescript-node -o gen-test",
    "doc:watch": "nodemon -e yaml --watch src/**/*.yaml --exec npm run doc:openapi",
    "doc": "ts-node -r tsconfig-paths/register src/doc.ts"
  },
  "author": "karam barakat",
  "license": "MIT",
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.17.3",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "lodash.merge": "^4.6.2",
    "mongodb": "^4.14.0",
    "mongoose": "^6.2.7",
    "morgan": "^1.10.0",
    "passport": "^0.5.2",
    "passport-google-oauth20": "^2.0.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "redoc": "^2.0.0",
    "redoc-express": "^1.0.0",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/plugin-transform-modules-commonjs": "^7.20.11",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.2",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/mocha": "^10.0.1",
    "@types/mongodb": "^4.0.7",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.45",
    "@types/passport": "^1.0.7",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-http-bearer": "^1.0.37",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@types/yup": "^0.29.13",
    "babel-plugin-tsconfig-paths-module-resolver": "^1.0.4",
    "dotenv-cli": "^7.0.0",
    "glob": "^8.1.0",
    "jest": "^27.5.1",
    "jest-extended": "^3.2.4",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "ts-jest": "^27.1.4",
    "ts-node-dev": "^1.1.8",
    "types": "file:../types",
    "yaml": "^2.2.1"
  }
}