{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "lint": {
      "outputs": []
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "test": {
      "dependsOn": []
    },
    "deploy": {
      "dependsOn": ["build", "test", "^deploy"]
    },
    "dev": {
      "dependsOn": ["^openapi#build"],
      "cache": false,
      "persistent": true
    }
  }
}
