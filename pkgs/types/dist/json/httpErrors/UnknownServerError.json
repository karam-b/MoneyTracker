{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://expamle.com/httpErrors/UnknownServerError",
  "type": "object",
  "properties": {
    "status": {
      "const": 500
    },
    "name": {
      "const": "UnknownServerError"
    },
    "message": {
      "type": "string"
    },
    "details": {
      "anyOf": [
        {
          "type": "null"
        },
        {
          "type": "object",
          "properties": {
            "dev": {
              "type": "string"
            }
          }
        }
      ]
    }
  },
  "required": [
    "status",
    "name",
    "message",
    "details"
  ],
  "additionalProperties": false
}