{
  "name": "my-pocket",
  "version": "1.0.0",
  "description": "Easy to use money wallet",
  "main": "backend/server.js",
  "scripts": {
    "dev:frontend": "npm run dev --prefix frontend",
    "dev:backend": "npm run dev  --prefix backend",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "terraform:init": "npm run terraform -- init",
    "terraform:plan": "npm run terraform -- plan -var-file=secrets.tfvars.json",
    "terraform:apply": "npm run terraform -- apply -var-file=secrets.tfvars.json -auto-approve",
    "terraform": "terraform -chdir=terraform"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karambarakat/MoneyTracker.git"
  },
  "author": "Karam Barakat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/karambarakat/MoneyTracker/issues"
  },
  "homepage": "https://github.com/karambarakat/MoneyTracker#readme"
}